# n8n Individual Agent Webhooks (Strands calls these directly -- no orchestrator)
N8N_FUEL_WEBHOOK_URL=http://localhost:5678/webhook/fuel
N8N_COLES_WEBHOOK_URL=http://localhost:5678/webhook/coles
N8N_MAPS_WEBHOOK_URL=http://localhost:5678/webhook/maps

# AWS Bedrock Configuration (for Strands Agent)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Bedrock Model ID (optional, defaults to Claude Sonnet)
# BEDROCK_MODEL_ID=us.anthropic.claude-sonnet-4-20250514-v1:0

# n8n Main Webhook URL (handles routing to all agents: Coles, Fuel, Maps)
# This is the ONE webhook that receives all requests and routes them internally
# For local development with n8n running on localhost:
N8N_MAIN_WEBHOOK_URL=http://localhost:5678/webhook/chat

# For n8n Cloud, use your cloud instance URL:
# N8N_MAIN_WEBHOOK_URL=https://your-instance.app.n8n.cloud/webhook/chat

# Legacy names (for backwards compatibility):
# N8N_GROCERY_WEBHOOK_URL=http://localhost:5678/webhook/chat
# N8N_TRANSPORT_WEBHOOK_URL=http://localhost:5678/webhook/chat

# n8n Configuration
N8N_TIMEOUT=30
